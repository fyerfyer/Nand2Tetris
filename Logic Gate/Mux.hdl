/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel, out = Notsel);
    And(a = Notsel, b = a, out = aOut);
    And(a = sel, b = b, out = bOut);
    Or(a = aOut, b = bOut, out = out);
}